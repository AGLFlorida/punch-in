{
  "extends": "../../tsconfig.renderer.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "../../src/renderer/**/*.ts",
    "../../src/renderer/**/*.tsx",
    "../../src/**/*.d.ts",
    ".next/types/**/*.ts"
  ]
}
